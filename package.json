{
  "name": "howto.technology",
  "version": "0.0.1",
  "description": "HowTo.Technology website",
  "scripts": {
    "dev": "export NODE_ENV=development && astro dev",
    "dev:h": "export NODE_ENV=development && astro dev --host",
    "start": "npm run dev",
    "start:h": "npm run dev:h",
    "build": "export NODE_ENV=production && astro build",
    "build:p": "npm run build && npm run preview",
    "build:ph": "npm run build && npm run preview:h",
    "preview": "astro preview",
    "preview:h": "astro preview --host"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottaw66/howto.technology.git"
  },
  "keywords": [
    "astro",
    "site",
    "howto",
    "technology"
  ],
  "author": "Scott Willsey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scottaw66/howto.technology/issues"
  },
  "homepage": "https://github.com/scottaw66/howto.technology#readme",
  "dependencies": {
    "astro": "^4.16.14",
    "astro-expressive-code": "^0.38.3",
    "astro-font": "^0.1.81",
    "astro-pagefind": "^1.6.0",
    "date-fns": "^4.1.0",
    "markdown-it": "^14.1.0",
    "normalize.css": "^8.0.1",
    "rehype-accessible-emojis": "^0.3.2",
    "remark-toc": "^9.0.0",
    "sanitize-html": "^2.13.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1"
  }
}
